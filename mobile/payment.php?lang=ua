            $("#dial-send").dialog({
                modal: true,
                autoOpen: false,
                buttons: {
                    "OK": function() {
                        $( this ).dialog( "close" );
                        var run = $("#dial-send").attr("run");
                        if (run != '') {
                            eval(run);
                        }
                    }
                }
            });
            $("#dial-conf").dialog({
                modal: true,
                autoOpen: false,
                buttons: {
                    "Да": function() {
                        $( this ).dialog( "close" );
                        var run = $("#dial-conf").attr("run");
                        if (run != '') {
                            eval(run);
                        }
                    },
                    "Нет": function() {
                        $( this ).dialog( "close" );
                    }
                }
            });
            $("#dial-wait").dialog({
                modal: true,
                autoOpen: false,
                dialogClass: "no-close",
            });
            $(".ui-dialog-buttonset > button").css("padding","6px 30px");

            function showwait(title, mess, run) {
                if (run === undefined) {
                    run = '';
                }
                $("#idlasterror").val("sw;t=" + title + ";m=" + mess + ";r=" + run);
                $("#dial-wait").dialog( "option", "title", title);
                $("#dial-wait").html(mess);
                $("#dial-wait").attr("run", run);
                $("#dial-wait").dialog("open");
            }

            function showmes(title, mess, run) {
                if (run === undefined) {
                    run = '';
                }
                $("#idlasterror").val("sm;t=" + title + ";m=" + mess + ";r=" + run);
                $("#dial-send").dialog( "option", "title", title);
                $("#dial-send").html(mess);
                $("#dial-send").attr("run", run);
                $("#dial-send").dialog("open");
            }

            function showconf(title, mess, run) {
                if (run === undefined) {
                    run = '';
                }
                $("#idlasterror").val("sc;t=" + title + ";m=" + mess + ";r=" + run);
                $("#dial-conf").dialog( "option", "title", title);
                $("#dial-conf").html(mess);
                $("#dial-conf").attr("run", run);
                $("#dial-conf").dialog("open");
            }

            function addzero(str, num) {
                str = '' + str;
                while (str.length < num) {
                    str = '0' + str;
                }
                return str;
            }
            function sendcodeform() {
                var sendcode = $("#usercode").val();
                var newtr = new Date();
                sendcode += '/' + addzero(newtr.getFullYear(), 4) + addzero(newtr.getMonth() + 1, 2) + addzero(newtr.getDate(), 2) + addzero(newtr.getHours(), 2) + addzero(newtr.getMinutes(), 2) + addzero(newtr.getSeconds(), 2);
                $("#hidcode").val(sendcode);
                $("#UPC-form").submit();
            };

            function notifyform(mess) {
                showconf(setError['notice'], mess, 'sendcodeform()');
            }

            $(document).ready(function(){

                $("#UPC-send").click(function(e) {
                    var totalSum = $("#usersum").val();
                    totalSum = totalSum.replace(/\,/g, ".");
                    totalSum = totalSum.replace(/\s|\t|\n|\r/g, "");
                    $("#usersum").val(totalSum);
                    var boolSum = isNaN(totalSum);
                    var fullcode = $("#usercode").val();
					fullcode = fullcode.replace(/\\/g, "/");
                    var z = fullcode.indexOf("/");
                    var usercode = fullcode.split("/");
                    var boolCode = false;
                    var i;
                    for (i = 0; i < usercode.length; i++) {
                        usercode[i] = $.trim(usercode[i]);
                        if (isNaN(usercode[i])) {
                            boolCode = true;
                        }
                    }
                    if((boolSum == true) || (totalSum <= 0) ){
                        showmes(setError['notice'], setError['e17']);
                        return false;
                    } else if ((z == -1) || (boolCode == true)) {
                        showmes(setError['notice'], setError['e16']);
                        return false;
                    }

                    showwait(setError['wait'], setError['e14']);

                    var args = new Object();
                    args['usercode'] = usercode[0];
                    args['fullcode'] = fullcode;

                    $.post('https://www.clearwater.ua/order/_system/jsclient.php', args, function(data) {
                        $("#dial-wait").dialog("close");
                        var sep = data.indexOf("|");
                        var rescode = data.split("|");
                        if ((sep != -1) && (rescode[0] == 'OK')) {
                            var dialogText = setError['p1'];
                            dialogText = dialogText.replace("[code]", rescode[1]);
                            dialogText = dialogText.replace("[name]", rescode[2]);

                            showconf(setError['notice'], dialogText, 'sendcodeform()');
                        } else if ((sep != -1) && (rescode[0] == 'NOTICE')) {
                            var dialogText = setError['p1'];
                            dialogText = dialogText.replace("[code]", rescode[1]);
                            dialogText = dialogText.replace("[name]", rescode[2]);

                            showconf(setError['notice'], setError['p2'], 'notifyform("' + dialogText + '")');
                        } else if ((sep != -1) && (rescode[0] == 'NEW')) {
                            var dialogText = setError['p3'];
                            dialogText = dialogText.replace("[code]", rescode[1]);

                            var dialogText2 = setError['p4'];
                            dialogText2 = dialogText2.replace("[code]", rescode[1]);

                            showconf(setError['notice'], dialogText, 'notifyform("' + dialogText2 + '")');
                        } else if ((sep != -1) && (rescode[0] == 'ERROR')) {
                            showmes(setError['notice'], setError[rescode[1]]);
                        } else {
                            showmes(setError['notice'], setError['e10']);
                        }
                    }).fail(function() {
                        $("#dial-wait").dialog("close");
                        showmes(setError['notice'], setError['e10']);
                    });
                    return false;
                });

                $(".input-return").click(function() {
                    var v = $("#input-text").val();
                    var attribute = $(this).attr("data-input");
                    $("[data-header='"+attribute+"'] ").val(v);
                    $("#modal-input").modal();
                });
                var id;
                $(document).on( "click",".calc-challenge", function() {
                     id = $(this).attr("id");
                  var title = $(this).attr("data-header");
                  var header ='<div class="close" data-dismiss="modal">&times;</div>';
                  header+='<h4 class="modal-title calc-title">';
                  header+=title;
                  header+='</h4>';
                  $("#calculator .modal-header").html(header);
                  $("#calculator").modal();


                });
                var FlagNewNum = false;
                $('.calc-figure').click(function(){
                  var num = $(this).val();
                  if(FlagNewNum){
                    $('#calculator-field').val(num);
                    FlagNewNum = false;
                  }
                  else{
                        var t = $('#calculator-field').val() + num ;
                        $('#calculator-field').val(t);
                  }
                });
                $(document).on("click",".calc-return",function(){

                  calcvalue = $('#calculator-field').val();

                  var k = $('#calculator-field').val();
                  $('#'+id).val(calcvalue);
                  $('#calculator-field').val("");
                });

                $('.calc-clear').click(function(){
                  var str = $('#calculator-field').val();
                  var lastSymbol = str.length-1;
                  var newStr = str.substring(0,lastSymbol);
                  $('#calculator-field').val(newStr);


                });
                $('.calc-delete').click(function(event) {
                    $('#calculator-field').val("");
                });
                $('#usercode').click(function() {
                   $(".dot-btn").addClass('inv');
                   $(".slash-btn").removeClass('inv');

                });
                $("#calculator").on("hidden.bs.modal",function(){
                     $(".dot-btn").removeClass('inv');
                   $(".slash-btn").addClass('inv');
                });


            });
        </script>
    </body>
</html>