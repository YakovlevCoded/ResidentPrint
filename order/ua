	function setInVal() {
			if ($.trim(setInput["orcode"]) != '') {
				$("#idcode").val(setInput["orcode"]);
			}
			if ($.trim(setInput["orcity"]) != '') {
				$("#idcity").val(setInput["orcity"]);
			}
			if ($.trim(setInput["orstrit"]) != '') {
				$("#idst").val(setInput["orstrit"]);
			}
			if ($.trim(setInput["orhouse"]) != '') {
				$("#idhouse").val(setInput["orhouse"]);
			}
			if ($.trim(setInput["oroffice"]) != '') {
				$("#idoffice").val(setInput["oroffice"]);
			}
			if ($.trim(setInput["orname"]) != '') {
				$("#idcname").val(setInput["orname"]);
			}
			if ($.trim(setInput["ortel"]) != '') {
				$("#idctel").val(setInput["ortel"]);
			}
			if ($.trim(setInput["ortime"]) != '') {
				$("#iddeltime").val(setInput["ortime"]);
			}
			if ($.trim(setInput["ordate"]) != '') {
				$("#iddeldate").val(setInput["ordate"]);
			}
			if ($.trim(setInput["orcw"]) != '') {
				$("#idcw").val(setInput["orcw"]);
			}
			if ($.trim(setInput["orcwf"]) != '') {
				$("#idcwf").val(setInput["orcwf"]);
			}
			if ($.trim(setInput["orcwi"]) != '') {
				$("#idcwi").val(setInput["orcwi"]);
			}
			if ($.trim(setInput["orconfsms"]) != '') {
				$("#chksms").attr('checked', 'checked');
				$("#idconfsms").val(setInput["orconfsms"]);
				$("#divchksms").show();
			}
			if ($.trim(setInput["orconfemail"]) != '') {
				$("#chkmail").attr('checked', 'checked');
				$("#idconfemail").val(setInput["orconfemail"]);
				$("#divchkmail").show();
			}
			if ($.trim(setInput["orconftel"]) != '') {
				$("#chktel").attr('checked', 'checked');
				$("#idconftel").val(setInput["orconftel"]);
				$("#divchktel").show();
			}
			if ($.trim(setInput["orcomm"]) != '') {
				$("#idcomm").val(setInput["orcomm"]);
			}
			if ($.trim(setInput["price-st"]) != '') {
				$("#price-st").val(setInput["price-st"]);
				parseT('', '');
				viewProd();

			}

	}
	function initorderjsp() {

