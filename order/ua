	arFunInit.push(initorderjsp);

	function errorLog() {
        var args = new Object();
        $("input, textarea").each(function() {
            var tname = $(this).attr('name');
            var tval = $(this).val();
            tval = tval.replace("\n", " ");
            args[tname] = tval;
        });
        args['cli-time'] = new Date();

        $.post('https://www.Чистовъ.ua/order/_system/jserror.php', args, function(data) {
            if (data.indexOf("OK") != -1) {

            }
        }).fail(function() {

        });

    }
	function sendForm() {
		ga('send', 'event', 'button', 'click', 'zakazsend');

        showwait(setError['wait'], setError['e14']);

		var args = new Object();
		$("input, textarea").each(function() {
			var tname = $(this).attr('name');
			var tval = $(this).val();
			tval = tval.replace("\n", " ");
			args[tname] = tval;
		});

        $.post('https://www.Чистовъ.ua/order/_system/jsorder.php', args, function(data) {
            $("#dial-wait").dialog("close");
            if (data.indexOf("OK") != -1) {
                $("input.orderin, textarea.orderarea").each(function() {
                    $(this).val('');
                });
                objPrice = Object();
                viewProd();

                if ((data.indexOf("MAIL") != -1) || (data.indexOf("POST") != -1)) {
                    showmes(setError['notice'], setError['e13']);
                    errorLog();
                } else {
                    showmes(setError['ok'], setError['e11']);
                }
            } else {
                if (data.indexOf("FIELD") != -1) {
                    var errarr = data.split(".");
                    if (errarr[0].indexOf("e") != -1) {
                        showmes(setError['error'], setError[errarr[0]]);
                        errorLog();
                    }
                } else {
                    showmes(setError['error'], setError['e10']);
                    errorLog();
                }
            }
        }).fail(function() {
            $("#dial-wait").dialog("close");
            showmes(setError['error'], setError['e10']);
            errorLog();
        });

    }

	function showwait(title, mess, run) {
		if (run === undefined) {
			run = '';
	    }
	    $("#idlasterror").val("sw;t=" + title + ";m=" + mess + ";r=" + run);
		$("#dial-wait").dialog( "option", "title", title);
		$("#dial-wait").html(mess);
		$("#dial-wait").attr("run", run);
		$("#dial-wait").dialog("open");
	}

	function showmes(title, mess, run) {
		if (run === undefined) {
			run = '';
	    }
        $("#idlasterror").val("sm;t=" + title + ";m=" + mess + ";r=" + run);
		$("#dial-send").dialog( "option", "title", title);
		$("#dial-send").html(mess);
		$("#dial-send").attr("run", run);
		$("#dial-send").dialog("open");
	}

	function showconf(title, mess, run) {
		if (run === undefined) {
			run = '';
	    }
        $("#idlasterror").val("sc;t=" + title + ";m=" + mess + ";r=" + run);
		$("#dial-conf").dialog( "option", "title", title);
		$("#dial-conf").html(mess);
		$("#dial-conf").attr("run", run);
		$("#dial-conf").dialog("open");

		errorLog();

	}

	function fullCheck(fieldN, isSendB) {

		var now = new Date();
		var today = new Date(now.getFullYear(), now.getMonth(), now.getDate());
		var todayp1 = new Date(now.getFullYear(), now.getMonth(), now.getDate() + 1);
		var todayp2 = new Date(now.getFullYear(), now.getMonth(), now.getDate() + 2);
		var todayp3 = new Date(now.getFullYear(), now.getMonth(), now.getDate() + 3);
		var totime = now - today;
		var today1545 = new Date(now.getFullYear(), now.getMonth(), now.getDate(), 15, 45);
		var today1945 = new Date(now.getFullYear(), now.getMonth(), now.getDate(), 19, 45);
		var ordateVal = $("input[name='ordate']").val();
		var ordate = strToDate(ordateVal);
		 var ordateValue = Date.parse(ordate);
		 var specialOrdate = new Date(2017, 0, 8, 0, 0, 0, 0);//форматируем Дату доставки
  var specialOrdate = Date.parse(specialOrdate);
		var ortime = $.trim($("input[name='ortime']").val());
		var mincw = 2;
		var allcw = 0;
		var tcw = parseInt($.trim($("input[name='orcw']").val()));
		if (!isNaN(tcw)) {
			allcw += tcw;
		}
		tcw = parseInt($.trim($("input[name='orcwf']").val()));
		if (!isNaN(tcw)) {
			allcw += tcw;
		}
		tcw = parseInt($.trim($("input[name='orcwi']").val()));
		if (!isNaN(tcw)) {
			allcw += tcw;
		}

		if (fieldN == 'ordate') {
            var xday = new Date("07/02/2018");
            var curmenu = $("input[name='timelist']").attr("cur");
			if ((ordate.getDay() == 6) || (inDateList(ordate, setDuty))) {
				$("input[name='timelist']").attr("cur", "menutimemin");
			} else if (ordate < xday) {
                $("input[name='timelist']").attr("cur", "menutimemax");
			} else {
                $("input[name='timelist']").attr("cur", "menutimenew");
            }
			if (curmenu != $("input[name='timelist']").attr("cur")) {
                $("input[name='ortime']").val('');
            }

			if (ordate < today) {
                showmes(setError['error'], setError['e2']);
				return true;
			} else if (ordate.getDay() == 0 && ordateValue != specialOrdate) {
                showmes(setError['error'], setError['e3']);
				return true;
			} else if (inDateList(ordate, setHoliday)) {
                showmes(setError['error'], setError['e4']);
				return true;
			} else if (inDateList(ordate, setCloseday)) {
                showmes(setError['error'], setError['e5']);
				return true;
			} else if (ordate - today == 0) {
                showmes(setError['error'], setError['e5']);
				return true;
			} else if (inDateList(today, setHoliday) && (ordate - todayp1 == 0)) {
                showmes(setError['error'], setError['e6']);
				return true;
			} else if ((now.getDay() == 0) && (ordate - todayp1 == 0) && (now - today1545 > 0)) {
                showmes(setError['error'], setError['e6']);
				return true;
			} else if (inDateList(today, setDuty) && (ordate - todayp1 == 0) && (now - today1545 > 0)) {
                showmes(setError['error'], setError['e6']);
				return true;
			} else if ((ordate - todayp1 == 0) && (now - today1945 > 0)) {
                showmes(setError['error'], setError['e6']);
				return true;
			}
		}
		if (isSendB) {
			if (($.trim($("input[name='orstrit']").val()) == '') || ($.trim($("input[name='orhouse']").val()) == '') || ($.trim($("input[name='orname']").val()) == '') || ($.trim($("input[name='ortel']").val()) == '')) {
                showmes(setError['error'], setError['e12']);
				return true;
			} else if (ordate == false) {
                showmes(setError['error'], setError['e1']);
				return true;
			} else if (ortime == '') {
                showmes(setError['error'], setError['e7']);
				return true;
			} else if (allcw == 0) {
                showmes(setError['error'], setError['e9']);
				return true;
			} else if ((allcw > 0) && (allcw < mincw)) {
                showconf(setError['notice'], setError['e8'], 'sendForm()');
				return true;
			}
		}
		return false;
	}

	function inDateList(cur, list) {
		cur = new Date(cur);
		var sy = cur.getFullYear();
		var sm = '' + (cur.getMonth() + 1);
		while (sm.length < 2) {
			sm = '0' + sm;
		}
		var sd = '' + cur.getDate();
		while (sd.length < 2) {
			sd = '0' + sd;
		}
		var s = sy + '-' + sm + '-' + sd;
		var r = list.indexOf(s);
		if (r == -1) {
			return false;
		}
		return true;
	}

	function strToDate(str) {
		var tmpdate = $.trim(str);
		var arrdate = tmpdate.split('.');
		if (arrdate.length == 3) {
			var tY = parseInt(arrdate[2]);
			if (isNaN(tY) || (tY < 2016) || (tY > 2050)) {
				return false;
			}
			var tM = parseInt(arrdate[1]);
			if (isNaN(tM) || (tM < 1) || (tM > 12)) {
				return false;
			}
			var tD = parseInt(arrdate[0]);
			if (isNaN(tD) || (tD < 1) || (tD > 31)) {
				return false;
			}
			return new Date(tY, tM - 1, tD);
		} else {
			return false;
		}
	}

	function closedial() {
		$("#dial-send").dialog("close");
		if (formReload) {
			location.reload();
		}
	}

	function parseT(addK, addV) {
		var prSTR;
		var prA;
		if (addK == '') {
			if (addV != 'del') {
				prSTR = $("#price-st").val();
				prA = prSTR.split("|");
				for (var i=0;i<prA.length;i++) {
					var arI = prA[i].split('=');
					objPrice[arI[0]] = arI[1];
				}
			}
		} else {
			objPrice[addK] = addV;
		}
		prSTR = '';
		for (i in objPrice) {
			if ((objPrice[i] != undefined) && (objPrice[i] != 0)) {
				if (prSTR != '') {
					prSTR += '|';
				}
				prSTR += i + '=' + objPrice[i];
			}
		}
		$("#price-st").val(prSTR);
	}

	function viewProd() {
		var tmp = '';
			tmp +='<table border="0" cellpadding="0" cellspacing="0" width="100%" height="100%">';
			tmp +='<tr>';
			tmp +='<td class="prodhead" align="center" nowrap>Назва</td>';
			tmp +='<td class="prodhead" align="center" nowrap width="42px">Кіл-ть</td>';
			tmp +='<td class="prodhead" align="center" nowrap width="40px">Ціна</td>';
			tmp +='<td class="prodhead" align="center" nowrap width="40px">Сума</td>';
			tmp +='<td class="prodheadlast" align="center" nowrap width="16px"> - </td>';
			tmp +='</tr>';
			for (i in objPrice) {
				curSelPrice = objPrice[i].split("::");
				tmp +='<tr>';
				tmp +='<td class="prodbody" align="left" nowrap><div class="prodhide">' + curSelPrice[0] + "</div></td>";
				tmp +='<td class="prodbody" align="right" nowrap><input  type="text" size="2" class="orderinprod" name="prodcoled" value="' + curSelPrice[2] + '" onchange="javascript:prodedit(' + "'" + i + "'" + ', this.value);"></td>';
				tmp +='<td class="prodbody" align="right" nowrap>' + curSelPrice[1] + "</td>";
				tmp +='<td class="prodbody" align="right" nowrap>' + parseInt(parseInt(curSelPrice[2]) * parseFloat(curSelPrice[1]) * 100) / 100 + "</td>";
				tmp +='<td class="prodbodylast" align="center" nowrap>' + '<a href="javascript:proddel(' + "'" + i + "'" + ');" style="color:red;text-decoration: none;" title="удалить">X</a>' + "</td>";
				tmp +='</tr>';
			}
				tmp +='<tr>';
				tmp +='<td class="prodbodyend" align="right" nowrap>&nbsp;</td>';
				tmp +='<td class="prodbodyend" align="right" nowrap>&nbsp;</td>';
				tmp +='<td class="prodbodyend" align="right" nowrap>&nbsp;</td>';
				tmp +='<td class="prodbodyend" align="right" nowrap>&nbsp;</td>';
				tmp +='<td class="prodbodyendlast" align="right" nowrap>&nbsp;</td>';
				tmp +='</tr>';
			tmp +='</table>';
		$("#tblprod").html(tmp);
		var t = $(".prodhide").parent().width();
		$(".prodhide").width(t - 5);
	}

	function prodedit(ind, cProd) {
		var cO = objPrice[ind].split('::');
		var resV = cO[0] + '::' + cO[1] + '::' + cProd;
		parseT(ind, resV);
		viewProd();
	}

	function proddel(ind) {
		delete objPrice[ind];
		parseT('', 'del');
		viewProd();
	}

	var hclose = function allclose() {
	// $(document).click(function() {
		var show = $("#dial-helpcode").dialog( "isOpen" );
		if (show) {
			$("#dial-helpcode").dialog( "close" );
		}
		var isDisabled = $( "#menutimemax" ).menu( "option", "disabled" );
		if (isDisabled == false) {
			$("#menutimemax").hide();
		}
        isDisabled = $( "#menutimenew" ).menu( "option", "disabled" );
        if (isDisabled == false) {
            $("#menutimenew").hide();
        }
		isDisabled = $( "#menutimemin" ).menu( "option", "disabled" );
		if (isDisabled == false) {
			$("#menutimemin").hide();
		}
		isDisabled = $( "#menuprod" ).menu( "option", "disabled" );
		if (isDisabled == false) {
			if (showMenu) {
				$("#menuprod").hide();
			}
		}
		if (showMenu) {
			$(document).unbind("click", hclose);
		}

		showMenu = true;
	}
	function verifyDate() {
		$(".ui-datepicker-calendar").find("tr").each(function() {

		});

	}
	function funchk() {
		$(".inhide").hide();
		$("input:checkbox").each(function () {
			var chkid = $(this).attr("id");
			if ($(this).prop("checked")) {
				var oldvar = $("#div" + chkid).find('input').val();
				if (oldvar == '') {
					var chkcopy = $("#div" + chkid).attr('copy');
					if (chkcopy != '') {
						var vcp = $("#" + chkcopy).val();
						$("#div" + chkid).find('input').val(vcp);
					}
				}
				$("#div" + chkid).show();
			} else {
				$("#div" + chkid).find('input').val("");
			}
		});
	}
</script>
