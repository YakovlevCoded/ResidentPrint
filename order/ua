		$("#menuprod").menu({
			select: function( event, ui ) {
				var resC = ui.item.find('a').attr('code');
				var resP = ui.item.find('a').attr('price');
				var resT = ui.item.find('a').attr('pname');
				if (resC != 'sub') {
					var resV = resT + '::' + resP + '::1';
					parseT(resC, resV);
					viewProd();
					$("#menuprod").hide();
				} else {
					var num = ui.item.attr('num');
					$(".sub-"+num).slideToggle();
					showMenu = false;
				}
				return false;
			}
		});
		$(".submenu").each(function() {
			var num = $(this).attr('num');
			$(".sub-"+num).hide();
		});
		$(".submenu").click(function(e) {
			$(".submenu").not(this).each(function() {
				var num = $(this).attr('num');
				$(".sub-"+num).hide();
			});
		});
		$(".subitem").mouseenter(function(e) {
			var offset = $(this).offset();
			$(this).find(".viewprice").each(function() {
				var pimg = $(this).attr("img");
				$("#idpopimg").attr("src", pimg).css('display','block');
			});
		}).mouseleave(function() {
			$("#idpopimg").hide();
		});
