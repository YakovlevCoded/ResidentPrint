		$("#idAutoSet").click(function() {
			if(preInput["orstrit"]==undefined&&preInput["orhouse"]==undefined&&preInput["orname"]==undefined&&preInput["ortel"]==undefined){
                showmes(setError['notice'], setError['e15']);
                errorLog();
				$(document).bind("click", hclose);
                return false;
			}
			if ($.trim(preInput["orcode"]) != '') {
				$("#idcode").val(preInput["orcode"]);
			}
			if ($.trim(preInput["orcity"]) != '') {
				$("#idcity").val(preInput["orcity"]);
			}
			if ($.trim(preInput["orstrit"]) != '') {
				$("#idst").val(preInput["orstrit"]);
			}
			if ($.trim(preInput["orhouse"]) != '') {
				$("#idhouse").val(preInput["orhouse"]);
			}
			if ($.trim(preInput["oroffice"]) != '') {
				$("#idoffice").val(preInput["oroffice"]);
			}
			if ($.trim(preInput["orname"]) != '') {
				$("#idcname").val(preInput["orname"]);
			}
			if ($.trim(preInput["ortel"]) != '') {
				$("#idctel").val(preInput["ortel"]);
			}
			if ($.trim(preInput["ortime"]) != '') {
				$("#iddeltime").val(preInput["ortime"]);
			}
			if ($.trim(preInput["orcw"]) != '') {
				$("#idcw").val(preInput["orcw"]);
			}
			if ($.trim(preInput["orcwf"]) != '') {
				$("#idcwf").val(preInput["orcwf"]);
			}
			if ($.trim(preInput["orcwi"]) != '') {
				$("#idcwi").val(preInput["orcwi"]);
			}
			if ($.trim(preInput["orconfsms"]) != '') {
				$("#chksms").attr('checked', 'checked');
				$("#idconfsms").val(preInput["orconfsms"]);
				$("#divchksms").show();
			}
			if ($.trim(preInput["orconfemail"]) != '') {
				$("#chkmail").attr('checked', 'checked');
				$("#idconfemail").val(preInput["orconfemail"]);
				$("#divchkmail").show();
			}
			if ($.trim(preInput["orconftel"]) != '') {
				$("#chktel").attr('checked', 'checked');
				$("#idconftel").val(preInput["orconftel"]);
				$("#divchktel").show();
			}
			if ($.trim(preInput["orcomm"]) != '') {
				// $("#idcomm").val(preInput["orcomm"]);
			}
			if ($.trim(preInput["price-st"]) != '') {
				$("#price-st").val(preInput["price-st"]);
				parseT('', '');

				var allPr = Array();
				$(".subitem a").each(function(index) {
					allPr.push($(this).attr("code"));
				});
				var notPr = '';
				var notPrdel = Array();
				for (i in objPrice) {
					if ((objPrice[i] != undefined) && (objPrice[i] != 0)) {
						if (allPr.indexOf(i) == -1) {
							var tip = objPrice[i].split("::");
							notPrdel.push(i);
							notPr = notPr + '<p>' + tip[0] + '</p>';
						}
					}
				}
				for (var i = 0; i < notPrdel.length; i++) {
					delete objPrice[notPrdel[i]];
					parseT('', 'del');
				}
				viewProd();
				if (notPr != '') {
                    showmes(setError['notice'], setError['e15']);
                    errorLog();
				}
			}
			return false;
		});

